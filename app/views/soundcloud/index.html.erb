<hr>
<% if user_signed_in? %>
  Hello <%= current_user.first_name %>,
  <%= link_to "Logout", sessions_path, method: :delete, data: {confirm: "are you sure you want to log out?"} %>
<% else %>
  <%= link_to "Login", new_session_path %> |
  <%= link_to "Sign Up", new_user_path %>
<% end %>
<hr>

<%= react_component "App", {songs: @tracks, current_user: current_user} %>

<script src="https://connect.soundcloud.com/sdk/sdk-3.1.1.js"></script>
